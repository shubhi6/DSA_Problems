//searching an element in unsorted array

#include<iostream>
using namespace std;

    int search(int n,int arr[],int x)
    {
        for(int i=0;i<n;i++)
		{
          if(arr[i]==x)
          return i;
        }
        return -1; 
    }

    int main()
    {
      int n;
      cout<<"size of array = ";
      cin>>n;

       cout<<"elements of array = ";
       int arr[n];
       for( int i=0;i<n;i++)
	   {
       cin>>arr[i];
        }
        cout<<"element to be searched = ";
        int x;
        cin>>x;
        cout<<search(n,arr,x)<<endl;

     
    }
    
